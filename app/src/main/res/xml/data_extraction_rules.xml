<?xml version="1.0" encoding="utf-8"?><!--
   Sample data extraction rules file; uncomment and customize as necessary.
   See https://developer.android.com/about/versions/12/backup-restore#xml-changes
   for details.
-->
<data-extraction-rules>
    <cloud-backup>
        <!-- Back up user data: favorites, alerts, and search history -->
        <include domain="database" path="stock_database"/>

        <!-- Exclude cache and temporary data -->
        <exclude domain="database" path="stock_database/StockCache.db"/>
        <exclude domain="file" path="cache/"/>
        <exclude domain="file" path="code_cache/"/>
    </cloud-backup>
    <!--
    <device-transfer>
        <include .../>
        <exclude .../>
    </device-transfer>
    -->
</data-extraction-rules>